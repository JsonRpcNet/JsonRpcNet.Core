<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <RootNamespace>JsonRpcNet</RootNamespace>
        <Version>0.1.12</Version>
        <Title>JsonRpcNet.Core</Title>
        <Description>Core library for adding JsonRpc support to your application</Description>
        <PackageProjectUrl>https://github.com/JsonRpcNet</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/JsonRpcNet/JsonRpcNet.Core/blob/master/LICENSE</PackageLicenseUrl>
        <PackageIconUrl>https://avatars1.githubusercontent.com/u/58533111</PackageIconUrl>
        <RepositoryUrl>https://github.com/JsonRpcNet/JsonRpcNet.Core</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>WebSocket JsonRpc RPC</PackageTags>
        <PackageReleaseNotes>Initial package</PackageReleaseNotes>
        <AssemblyVersion>0.1.12</AssemblyVersion>
        <FileVersion>0.1.12</FileVersion>
        <NeutralLanguage>en-US</NeutralLanguage>
        <Authors>Fabricio Bastian, Jonas Gottschau</Authors>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
      <PackageReference Include="NJsonSchema" Version="10.1.2" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="web\**\*" />
    </ItemGroup>

</Project>
